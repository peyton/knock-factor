    <string name="timesync_preference_screen_title" msgid="3022961447302189451">"Tidskorrigering för koder"</string>
    <string name="timesync_sync_now_connectivity_error_dialog_details" msgid="7427576990969917093">"Det gick inte att kontakta Googles server. Kontrollera dina nätverksinställningar och försök igen."</string>
